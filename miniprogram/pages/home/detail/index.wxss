.margin-b10
{
    margin-bottom: 10rpx;
}
.home-container-detial
{
    margin: 12rpx 8rpx;
    padding: 8rpx;

    border: 1rpx solid #D9D9D9;
}
.home-container-detial .title
{
    margin: 0px 10rpx;
    padding: 0px 8rpx;
    min-height: 2.58823529em;
    line-height: 2.58823529em;
        
    border-bottom: 1rpx solid #D9D9D9;
}
.home-container-detial .content
{
    margin: 10rpx 10rpx 0rpx 10rpx;
    padding: 0px 8rpx;
    /* line-height: 2.58823529em; */
    min-height: 2.58823529em;
}
.home-container-detial .content image
{
    width: 100%;
}
.home-container-detial .users
{
    display: flex;
    margin: 0rpx 8rpx 8rpx 8rpx;
    padding: 0px 8rpx;
    min-height: 2.58823529em;
    line-height: 2.58823529em;
}
.home-container-detial .users-img
{
    width: 80rpx;
    height: 80rpx;
        
    padding: 8rpx;
    border: 1rpx solid #D9D9D9;
    border-radius: 50%;
}
.home-container-detial .users image
{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
